#program to return factorial of 10 numbers
def factorial(n):
	if n==0 :
		result=1
	else:
		result=n*factorial(n-1)
	return result
	
#def factorial(n):
#	return 1 if n==0 else n*factorial(n-1)
#print(factorial(5))	

for i in range(10):
	print('factorial of {} is {}'.format(i,factorial(i)))
	
	
	
fibonacci series

n=10
n1,n2=0,1
count=0

while count < n:
	print(n1)
	nth=n1+n2
	n1=n2
	n2=nth
	count=count+1
	
