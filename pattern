#pattern programs
#n=int(input('enter any number:'))
n=5
for i in range(n):
	for j in range(i+1):
	  #print(j+1,end=' ')    # right angle triangle of 1,1 2,1 2 3 
	  #print(i+1,end=' ')    # right angle triangle of 1,2 2,3 3 3
	  #print('*',end=' ')    # right angle triangle of *   
	  print(n,end=' ')       # right angle triangle of 5    
	print()
